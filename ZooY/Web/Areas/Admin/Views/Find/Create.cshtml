@using Core.Constants
@using Web.Areas.Admin.ViewModels.Find
@model FindCreateVM

<form enctype="multipart/form-data" asp-action="create" asp-controller="find" method="post">
    <div class="form-group">
    <label asp-for="Title">Title</label>
      <textarea class="form-control" asp-for="Title"></textarea>
    <span asp-validation-for="Title"></span>
     </div>


    <div class="form-group">
        <label asp-for="MainPhoto">MainPhoto</label>
        <input class="form-control" asp-for="MainPhoto">
        <span asp-validation-for="MainPhoto"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create</button>
   <a class="btn btn-danger" asp-action="index" asp-controller="find">Cancel</a>
</form>

