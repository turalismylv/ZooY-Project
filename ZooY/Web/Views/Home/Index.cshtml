@if (Model.HotDeal!=null)
{
    <section data-aos="fade-up" style="background-image: url('/assets/img/@Model.HotDeal.PhotoName');" class="dealofday">
    <div class="divday">
        <h2 style="margin-bottom: 25px;">@Model.HotDeal.Title</h2>
        <h4>@Model.HotDeal.Text</h4>
        <div style="margin-top: 60px;">
            <div class="countdown-timer-container">
                <div style="display: none;" class="countdown-timer-values" id="year">@Model.HotDeal.Time.Year</div>
                <div style="display: none;" class="countdown-timer-values" id="mounth">@Model.HotDeal.Time.Month</div>
                <div class="countdown-timer-values" id="days">@Model.HotDeal.Time.Day</div>
                <div class="countdown-timer-values" id="hours">@Model.HotDeal.Time.Hour</div>
                <div class="countdown-timer-values" id="minutes">@Model.HotDeal.Time.Minute</div>
                <div class="countdown-timer-values" id="seconds">00</div>
                <div class="countdown-div">days</div>
                <div class="countdown-div">hours</div>
                <div class="countdown-div">minutes</div>
                <div class="countdown-div">seconds</div>
            </div>
        </div>
        <div class="slide-btn">
            <a class="btn common-btn btnday" asp-action="index" asp-controller="shop" tabindex="0"><span>Explore Now</span><i class="icon-next"></i></a>
        </div>
    </div>
</section>
    
}



